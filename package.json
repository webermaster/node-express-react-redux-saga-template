{
    "name": "node-template",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=production node ./bin/www",
        "dev": "cross-env NODE_ENV=development node ./bin/www",
        "clean": "rimraf public/styles/style.css public/scripts/bundle.js public/scripts/bundle.js.map",
        "lessc": "cross-env lessc ./public/styles/style.less ./public/styles/style.css",
        "package": "node_modules/cross-env/bin/cross-env.js NODE_ENV=production npm run lessc && webpack",
        "test": "mocha test",
        "test:dev": "mocha --watch test"
    },
    "description": "Node Template for NodeJS/React/Redux projects",
    "repository": {
        "type": "git",
        "url": "https://github.com/webermaster/node-template.git"
    },
    "dependencies": {
        "compression": "^1.6.2",
        "ejs": "^2.5.6",
        "express": "^4.15.3"
    },
    "devDependencies": {
        "chai": "^4.0.2",
        "chai-as-promised": "^7.0.0",
        "cross-env": "^5.0.1",
        "jsdom": "^11.0.0",
        "less": "^2.7.1",
        "less-middleware": "^2.2.0",
        "mocha": "^3.4.2",
        "piping": "^1.0.0-rc.4",
        "react": "^15.6.1",
        "react-addons-test-utils": "^15.6.0",
        "react-bootstrap": "^0.31.0",
        "react-dom": "^15.6.1",
        "react-router": "^3.0.5",
        "redux-logger": "^3.0.6",
        "redux": "^3.7.0",
        "react-redux": "^5.0.4",
        "redux-saga": "^0.15.3",
        "rimraf": "^2.6.1",
        "sinon": "^2.3.4",
        "sinon-chai": "^2.11.0",
        "uglify-js": "^2.8.0",
        "webpack": "^2.6.1",
        "webpack-dev-middleware-webpack-2": "^1.5.1",
        "babel-core": "^6.25.0",
        "babel-loader": "^7.0.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.24.1"
    },
    "engines": {
        "node": "^8.1.2",
        "npm": "^5.0.3"
    },
    "false": {}
}
