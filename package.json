{
    "name": "node-template",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=production node ./bin/www",
        "dev": "cross-env NODE_ENV=development node ./bin/www",
        "clean": "rimraf public/styles/style.css public/scripts/bundle.js public/scripts/bundle.js.map",
        "lessc": "cross-env lessc ./public/styles/style.less ./public/styles/style.css",
        "package": "cross-env NODE_ENV=production npm run lessc && webpack",
        "test": "mocha test",
        "test:dev": "mocha --watch test"
    },
    "description": "Node Template for NodeJS/React/Redux projects",
    "repository": {
        "type": "git",
        "url": "https://github.com/webermaster/node-template.git"
    },
    "dependencies": {
        "compression": "~1.6.1",
        "express": "~4.13.4",
        "ejs": "~2.5.2"
    },
    "devDependencies": {
        "chai": "~3.5.0",
        "chai-as-promised": "~5.3.0",
        "cross-env": "~1.0.8",
        "jsdom": "~8.5.0",
        "less": "~2.7.1",
        "less-middleware": "~2.1.0",
        "mocha": "~2.5.3",
        "piping": "~0.3.0",
        "react": "~15.1.0",
        "react-addons-test-utils": "~15.1.0",
        "react-bootstrap": "~0.30.7",
        "react-dom": "~15.1.0",
        "react-router": "~2.4.1",
        "redux-logger": "~2.8.1",
        "redux": "~3.6.0",
        "react-redux": "~5.0.1",
        "redux-saga": "^0.10.0",
        "rimraf": "~2.5.2",
        "sinon": "~1.17.4",
        "sinon-as-promised": "~4.0.0",
        "sinon-chai": "~2.8.0",
        "webpack": "~1.13.1",
        "webpack-dev-middleware": "~1.6.1",
        "babel-core": "~6.8.0",
        "babel-loader": "~6.2.4",
        "babel-polyfill": "~6.8.0",
        "babel-preset-es2015": "~6.6.0",
        "babel-preset-react": "~6.5.0",
        "babel-register": "~6.8.0"
    },
    "engines": {
        "node": "~7.7.3",
        "npm": "~3.9.3"
    }
}